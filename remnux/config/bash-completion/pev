. /etc/bash_completion.d/remnuxlib

_pestr() 
{
	local cur
	cur="${COMP_WORDS[COMP_CWORD]}"
	
	case "$cur" in -*)
		_parse_args "pestr --help"
	esac
}

complete -d -o default -F _pestr pestr

_pescan() 
{
	local cur
	cur="${COMP_WORDS[COMP_CWORD]}"
	
	case "$cur" in -*)
		_parse_args "pescan --help"
	esac
}

